{
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "preserve",
    "jsxImportSource": "react",
    "lib": ["es2017", "dom", "esnext"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "types": ["astro/client"],
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}